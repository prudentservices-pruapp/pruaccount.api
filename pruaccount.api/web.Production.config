<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <aspNetCore processPath="dotnet" arguments=".\bin\Debug\net5.0\Pruaccount.Api.dll" stdoutLogEnabled="false" hostingModel="inprocess" stdoutLogFile=".\logs\stdout" forwardWindowsAuthToken="false" xdt:Transform="Replace">
        <environmentVariables>
          <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
          <environmentVariable name="ASPNETCORE_HTTPS_PORT" value="44339" xdt:Transform="Remove" xdt:Locator="Match(name)"/>
        </environmentVariables>
      </aspNetCore>
    </system.webServer>
  </location>
</configuration>